{
  "name": "bikemeet",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:dev": "jest --watchAll",
    "test:coverage": "jest --watchAll=false --collect-coverage",
    "prepare": "husky install"
  },
  "dependencies": {
    "@expo-google-fonts/montserrat": "^0.2.3",
    "@expo-google-fonts/odibee-sans": "^0.2.3",
    "@expo/webpack-config": "^18.0.1",
    "@fontsource/odibee-sans": "^4.5.10",
    "@reduxjs/toolkit": "^1.9.4",
    "core-js": "^3.30.1",
    "expo": "~48.0.11",
    "expo-font": "^11.1.1",
    "expo-status-bar": "~1.4.4",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-expo": "^48.0.2",
    "jest-extended": "^3.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.71.7",
    "react-native-asset": "^2.1.1",
    "react-native-web": "~0.18.10",
    "react-redux": "^8.0.5",
    "redux": "^4.2.1",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-native": "^12.1.1",
    "@tsconfig/react-native": "^2.0.3",
    "@types/jest": "^29.5.0",
    "@types/react": "~18.0.14",
    "@types/react-native-asset": "^2.0.0",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-config-xo-typescript": "^0.57.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-native": "^4.0.0",
    "eslint-plugin-testing-library": "^5.10.3",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.7",
    "typescript": "^5.0.4"
  },
  "private": true,
  "jest": {
    "preset": "jest-expo",
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|@fortawesome/.)"
    ],
    "setupFilesAfterEnv": [
      "./src/jest.setup.ts",
      "@testing-library/jest-native/extend-expect"
    ]
  }
}
